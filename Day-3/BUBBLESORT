30	20	10
20	30	10
20	10	30

20	10
10	20



i=0--->1st ele
i=1--->2nd ele
i=2--->3rd ele
i=3--->4th ele
	for(i=0;i<n-1;i++){
		for(j=0;j<n-1-i;j++){
			if(arr[j]>arr[j+1]
			{
				temp=arr[j];
				arr[j]=arr[j+1];
				arr[j+1]=temp;
			}
		}
	}
